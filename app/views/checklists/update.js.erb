<% if @checklist.errors.blank? %>
  $('#checklist-name-<%= @checklist.id %>').text('<%= @checklist.name %>');
  $('.dropdown-menu').removeClass('show');
<% end %>
$('#edit-checklist-<%= @checklist.id %>').html('<%= j render "checklists/edit_form", checklist: @checklist %>');
$('#toastr').html('<%= j render "shared/toastr" %>');
